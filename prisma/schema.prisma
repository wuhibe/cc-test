generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model ReplyExamples {
    id              String  @id @default(cuid())
    username        String
    comment         String
    embedding       Float[]
    character_count Int
    reply           String

    created_at DateTime @default(now())
    updated_at DateTime @default(now()) @updatedAt

    @@map("reply_examples")
}
